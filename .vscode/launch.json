{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) rvemu starter",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/rvemu",
            "args": ["${BENCH_BINARY}"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "BENCH_BINARY",
                    "value": "${workspaceFolder}/prime"
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}